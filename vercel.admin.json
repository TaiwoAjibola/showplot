{
  "framework": "vite",
  "installCommand": "npm ci",
  "buildCommand": "npm run build:admin",
  "outputDirectory": "dist-admin",
  "routes": [
    { "src": "/api/(.*)", "dest": "https://showplot.onrender.com/api/$1" },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
