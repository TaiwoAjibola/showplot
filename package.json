{
  "name": "showplot",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev:client": "vite",
    "dev:admin": "vite --config vite.admin.config.js",
    "dev:server": "nodemon server.js",
    "dev": "concurrently -n client,admin,server -c auto \"npm:dev:client\" \"npm:dev:admin\" \"npm:dev:server\"",
    "db:up": "docker compose up -d mongo",
    "db:down": "docker compose down",
    "build": "npm run build:user && npm run build:admin",
    "build:user": "vite build",
    "build:admin": "vite build --config vite.admin.config.js",
    "lint": "eslint .",
    "preview": "vite preview",
    "preview:admin": "vite preview --config vite.admin.config.js",
    "start": "node server.js"
  },
  "dependencies": {
    "@react-oauth/google": "^0.13.4",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.6",
    "dotenv": "^17.3.1",
    "express": "^5.2.1",
    "google-auth-library": "^10.6.1",
    "html2canvas": "^1.4.1",
    "jsonwebtoken": "^9.0.3",
    "jspdf": "^4.2.0",
    "konva": "^10.2.0",
    "mongoose": "^9.2.1",
    "multer": "^1.4.4",
    "multer-gridfs-storage": "^5.0.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-konva": "^19.2.2",
    "react-router-dom": "^7.13.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "nodemon": "^3.1.14",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "vite": "^8.0.0-beta.13"
  },
  "overrides": {
    "vite": "^8.0.0-beta.13"
  }
}
